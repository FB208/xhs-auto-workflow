# AI 提供商选择: openai 或 gemini
AI_PROVIDER=gemini

# OpenAI 配置
OPENAI_API_KEY=your-api-key
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o-mini

# Gemini WebAPI 配置 (从浏览器复制完整 cookie)
GEMINI_COOKIE=your-cookie-string

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# 小红书 MCP 服务地址
XHS_MCP_URL=http://localhost:18060

# PicList 图床服务配置
PICLIST_URL=http://127.0.0.1:36677
PICLIST_KEY=

# 抖音配置 (Cookies 自动保存到此文件)
DOUYIN_COOKIE_FILE=douyin_cookies.json

# 视频号配置 (Cookies 自动保存到此文件)
WEIXIN_COOKIE_FILE=weixin_cookies.json

# 小红书配置 - Playwright 版本 (Cookies 自动保存到此文件)
XHS_COOKIE_FILE=xiaohongshu_cookies.json
